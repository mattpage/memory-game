{
  "extends": ["eslint-config-kyt"],
  "parser": "babel-eslint",
  "rules": {
    "no-return-assign": ["error", "except-parens"],
    "import/no-restricted-paths": "off",
    "import/no-named-as-default": "off",
    "react/require-default-props": "off",
    "prettier/prettier": [
      "error",
      {
        "printWidth": 100,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}
